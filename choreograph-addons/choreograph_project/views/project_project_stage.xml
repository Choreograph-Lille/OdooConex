<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="project_project_stage_view_tree_inherit" model="ir.ui.view">
        <field name="name">project.project.stage.view.tree.inherit</field>
        <field name="model">project.project.stage</field>
        <field name="inherit_id" ref="project.project_project_stage_view_tree" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sequence']" position="replace">
                <field name="sequence" options='{"type": "number", "step": 10}' />
            </xpath>
        </field>
    </record>
</odoo>