<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sol_compute_retribution_cost" model="ir.cron">
            <field name="name">C9H: compute retribution cost</field>
            <field name="model_id" ref="model_sale_order_line"/>
            <field name="state">code</field>
            <field name="code">model.search([('order_id.state_specific', 'not in', ['closed_won', 'adjustment', 'cancel'])])._compute_retribution_cost()</field>
            <field name="interval_number">6</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
                <field name="nextcall" eval="DateTime.strptime('01-01-2024','%d-%m-%Y')"/>
        </record>

    </data>
</odoo>