    <record id="standard_user" model="res.groups">
      <field name="name">Profile Standard</field>
      <field name="category_id" ref="module_category_my_model_profiles"/>
      <field name="implied_ids" eval="[(6, 0, [ref('maas_base.group_package_management'), ref('maas_base.group_start_modeling'),
