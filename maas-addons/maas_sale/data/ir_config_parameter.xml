<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        
        <record id="duration_availability_download" model="ir.config_parameter">
            <field name="key">duration.availability.download</field>
            <field name="value">30</field>
        </record>
        
        <record id="delay_notification_before_term_download" model="ir.config_parameter">
            <field name="key">delay.notification.before.term.download</field>
            <field name="value">5</field>
        </record>
        
        <record id="web_mymodel_url" model="ir.config_parameter" forcecreate="True">
            <field name="key">web.mymodel.url</field>
